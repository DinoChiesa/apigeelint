<RaiseFault name="RF-Unsupported">
  <FaultResponse>
    <Set>
      <Payload contentType="text/plain">Bad Request</Payload>
      <StatusCode>400</StatusCode>
      <ReasonPhrase>Bad Request</ReasonPhrase>
    </Set>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
